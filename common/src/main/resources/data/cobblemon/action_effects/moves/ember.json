{
  "timeline": [
    {
      "type": "add_holds",
      "holds": ["effects"]
    },
    {
      "type": "animation",
      "animation": ["ember", "fire", "spray", "special"],
      "delay": 1.25
    },
    {
      "type": "entity_particles",
      "effect": "cobblemon:ember_actor",
      "locators": ["special", "target"],
      "targetLocators": ["target"]
    },
    {
      "type": "entity_molang",
      "entityCondition": "q.entity.is_user == true",
      "expressions": [
        "q.sound('move.ember.actor');"
      ]
    },
    {
      "type": "pause",
      "pause": 0.5
    },
    {
      "type": "entity_particles",
      "entityCondition": "q.entity.is_user == false && q.missed(q.entity.uuid) == false",
      "effect": "cobblemon:ember_target",
      "locators": ["target"]
    },
    {
      "type": "entity_molang",
      "entityCondition": "q.entity.is_user == false && q.missed(q.entity.uuid) == false",
      "expressions": [
        "q.sound('move.ember.target');"
      ]
    },
    {
      "type": "remove_holds",
      "holds": ["effects"]
    },
    {
      "type": "pause",
      "pause": 1.8
    }
  ]
}