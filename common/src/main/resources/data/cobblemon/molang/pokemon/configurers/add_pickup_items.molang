'This script adds all the pickup items from the Pokemon (the species file) to the config struct';

t.poke = q.entity;
t.index = 0;
t.pickup_items = q.array();
for_each(t.item, t.poke.behaviour.item_interact.pickup_items, {
    t.pickup_items[t.index] = t.item;
    t.index = t.index + 1;
});
t.poke.config.pickup_items = t.pickup_items;