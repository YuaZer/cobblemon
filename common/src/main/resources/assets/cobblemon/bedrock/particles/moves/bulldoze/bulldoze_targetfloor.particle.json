{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:bulldoze_targetfloor",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/impact/impact_ground"
			}
		},
		"curves": {
			"variable.mudsize": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0, 0, 0, 1, 0.75, 0.5, 0.25, 0]
			}
		},
		"events": {
			"rumble": {
				"particle_effect": {
					"effect": "cobblemon:bulldoze_targetrumble",
					"type": "particle"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 120,
				"max_particles": 10000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1.25
			},
			"minecraft:emitter_shape_disc": {
				"offset": ["-(v.target_deltax*0.75)", 0.1, "-(v.target_deltaz*0.75)"],
				"radius": "math.clamp(v.target_deltaz*3,6,8)",
				"direction": ["math.random(-1,1)*0.1", -90, "math.random(-1,1)*0.1"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.25
			},
			"minecraft:particle_lifetime_events": {
				"timeline": {
					"0.10": "rumble"
				}
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0,3)*90"
			},
			"minecraft:particle_initial_speed": 1299,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["(v.particle_random_1-0.5)*2", 0, "(v.particle_random_3-0.5)*2"],
				"linear_drag_coefficient": 9
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(0.1+(v.particle_random_1*0.1))*v.mudsize", "(0.1+(v.particle_random_1*0.1))*v.mudsize"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 56,
					"flipbook": {
						"base_UV": [0, 16],
						"size_UV": [8, 8],
						"step_UV": [0, -8],
						"frames_per_second": 16,
						"max_frame": 3
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.1,
				"collision_radius": 0.02
			}
		}
	}
}