{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:seedbomb_target",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/fire/cloudyfire_white"
			}
		},
		"events": {
			"orbs": {
				"particle_effect": {
					"effect": "cobblemon:seedbomb_targetorbs",
					"type": "emitter"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 600,
				"max_particles": 8
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.05
			},
			"minecraft:emitter_lifetime_events": {
				"creation_event": "orbs"
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "0.45+((v.particle_random_4*0.08)*5)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "90*math.random_integer(0,3)"
			},
			"minecraft:particle_initial_speed": "((v.particle_random_4*0.4)+1.5)*2.5",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["(v.particle_random_1 * 32 - 16) * v.particle_age * v.particle_age", "(v.particle_random_2  * 32 - 16) * v.particle_age * v.particle_age", "(v.particle_random_3  * 32 - 16) * v.particle_age * v.particle_age"],
				"linear_drag_coefficient": 4
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.6, 0.6],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 160,
					"texture_height": 16,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [16, 16],
						"step_UV": [16, 0],
						"max_frame": 10,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"coefficient_of_restitution": 0.5,
				"collision_radius": 0.15
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.09": "#FFE3FFD5",
						"0.31": "#FFFEFF85",
						"0.6": "#FFFF7B4C",
						"1.0": "#FF442828"
					}
				}
			}
		}
	}
}