{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:shadowball_targetimpactsplotch",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/orb/xsfadeorb"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 360,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.1
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 0, 0],
				"radius": 0.05,
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_speed": "math.random(4.1,6.3)*2.7",
			"minecraft:particle_motion_dynamic": {
				"linear_drag_coefficient": 5
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.125-(v.particle_random_1*0.075)", "0.125-(v.particle_random_1*0.075)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 45,
					"texture_height": 5,
					"flipbook": {
						"base_UV": [10, 0],
						"size_UV": [5, 5],
						"step_UV": [5, 0],
						"frames_per_second": 12,
						"max_frame": 5,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ff000000",
						"0.7": "#FF000000",
						"1.0": "#00000000"
					}
				}
			}
		}
	}
}