				"max_lifetime": "math.random(0.3, 0.4)"
			},
			"minecraft:particle_initial_speed": "math.random(6,9)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, -9.8, 0],
				"linear_drag_coefficient": 5
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.0625*((v.particle_age / v.particle_lifetime)-1)*(-1)", "0.0625*((v.particle_age / v.particle_lifetime)-1)*(-1)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 8,
					"uv": [0, 0],
					"uv_size": [8, 8]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_random_4",
					"gradient": {
						"0.0": "#FFD1F5FF",
						"1.0": "#FF8BD0FF"
					}
				}
			}
		}
	}
