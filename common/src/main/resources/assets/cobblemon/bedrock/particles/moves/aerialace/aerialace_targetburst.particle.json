{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:aerialace_targetburst",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/white"
			}
		},
		"curves": {
			"variable.scale": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [1, 0]
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 150,
				"max_particles": 200
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.125
			},
			"minecraft:emitter_shape_point": {
				"offset": ["-v.emitter_age*10-0.25", "(v.emitter_age*10)-0.25", "(v.emitter_age*10/2)-0.25"],
