{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:razorleaf_actorexcess",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/grass/razorleaf"
			}
		},
		"curves": {
			"variable.leafsize": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.9, 1, 1, 1, 1, 0.9, 0.6, 0]
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 25,
				"max_particles": 10000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.2
			},
			"minecraft:emitter_shape_point": {
				"direction": ["math.clamp(v.entity_width,1,2.5)*(math.sin(v.emitter_age*1800)*1.5)", "(math.clamp(v.entity_height,1,3)*(1.15+math.sin(v.emitter_age*4880)))*1.25", "math.clamp(v.entity_width,1,2.5)*(math.cos(v.emitter_age*1800)*1.5)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.round(v.particle_random_4*3)*90"
			},
			"minecraft:particle_initial_speed": "(math.clamp(v.entity_width,1,2.5)*4)+(v.particle_random_4*0.5)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, "-math.clamp(v.entity_height*1.5,2,4)*1.3", 0],
				"linear_drag_coefficient": "0.5+(math.clamp(v.entity_width,1,2.5)*0.5)"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.2*v.leafsize", "0.2*v.leafsize"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 64,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 11,
						"max_frame": 7,
						"loop": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.4,
				"coefficient_of_restitution": 0.4,
				"collision_radius": 0.01
			}
		}
	}
}