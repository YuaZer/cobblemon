{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:leechlife_actortrail",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/tinydust"
			}
		},
		"curves": {
			"variable.crtrailsize": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1, 1, 1, 0.9, 0.5, 0]
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.45
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 0.15,
				"surface_only": true,
				"direction": ["(v.particle_random_1-0.5)*0.75", "((v.particle_random_1-0.5)*0.75)-((v.target_deltay*0.75)/v.target_distance)", 1]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.4
			},
			"minecraft:particle_initial_speed": 1.85,
			"minecraft:particle_motion_dynamic": {
				"linear_drag_coefficient": 1
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.0275*(v.crtrailsize)", "0.0275*(v.crtrailsize)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 12,
					"texture_height": 15,
					"flipbook": {
						"base_UV": ["math.round(v.particle_random_1*2)*3", 0],
						"size_UV": [3, 3],
						"step_UV": [3, 0],
						"frames_per_second": 3.5,
						"max_frame": 2,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#46FF4733",
						"0.1": "#AFFF4733",
						"0.2": "#DFFF5D4B",
						"0.45": "#DFFF5D4B",
						"0.7": "#FFFFA951",
						"1.0": "#46FFDF6C"
					}
				}
			}
		}
	}
}