{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:icywind_target",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/impact/impact_ice"
			}
		},
		"curves": {
			"variable.iw2sizing": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.53, 0.84, 1, 1, 1, 1, 1, 1, 1]
			}
		},
		"events": {
			"crash": {
				"particle_effect": {
					"effect": "cobblemon:icywind_targetbreak",
					"type": "emitter"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 300,
				"max_particles": 2000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.2
			},
			"minecraft:emitter_lifetime_events": {
				"creation_event": "crash",
				"timeline": {
					"0.05": "crash"
				}
			},
			"minecraft:emitter_shape_sphere": {
				"radius": "q.entity_radius*0.55",
				"surface_only": true,
				"direction": ["math.random(-12.5,12.5)", "math.random(-12.5,12.5)", "math.random(-12.5,12.5)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(0.9,1.7)*0.7"
			},
			"minecraft:particle_initial_speed": "math.random(1.25,4.75)*2.5",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.sin(v.particle_age*((v.particle_random_2-0.5)*(360+(v.particle_random_2*180))))*3.5", "math.cos(v.particle_age*((v.particle_random_3-0.5)*(360+(v.particle_random_2*180))))*3.5", "math.sin(v.particle_age*((v.particle_random_4-0.5)*(360+(v.particle_random_2*180))))*3.5"],
				"linear_drag_coefficient": 4
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.1*v.iw2sizing", "0.1*v.iw2sizing"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 56,
					"flipbook": {
						"base_UV": [0, "16+math.round(v.particle_random_2*2)*8"],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 7,
						"max_frame": 3,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#00316BFF",
						"0.15": "#9FD7EBFF",
						"0.61": "#BFA5D2FF",
						"1.0": "#00316BFF"
					}
				}
			}
		}
	}
}