{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:magicalleaf_target",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/minihit"
			}
		},
		"events": {
			"spark": {
				"particle_effect": {
					"effect": "cobblemon:magicalleaf_targetspark",
					"type": "particle"
				}
			},
			"leaf": {
				"particle_effect": {
					"effect": "cobblemon:magicalleaf_targetexcess",
					"type": "particle"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": "25*math.clamp(v.entity_height,0.75,1)",
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.4
			},
			"minecraft:emitter_shape_sphere": {
				"radius": "math.clamp(v.entity_height*0.5,0.5,1)",
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.16
			},
			"minecraft:particle_lifetime_events": {
				"creation_event": ["spark", "leaf"]
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0,1)*90"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.275-(v.particle_random_2*0.075)", "0.275-(v.particle_random_2*0.075)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 39,
					"texture_height": 26,
					"flipbook": {
						"base_UV": [0, "math.round(v.particle_random_2)*13"],
						"size_UV": [13, 13],
						"step_UV": [13, 0],
						"frames_per_second": 16,
						"max_frame": 3
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FFFF6743",
						"0.25": "#FFFFF44A",
						"0.55": "#FF567FFF",
						"0.85": "#FFFE48FF",
						"1.0": "#FFFF6743"
					}
				}
			}
		}
	}
}