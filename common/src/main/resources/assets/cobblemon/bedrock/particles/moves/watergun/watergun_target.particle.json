{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "cobblemon:watergun_target",
      "basic_render_parameters": {
        "material": "particles_blend",
        "texture": "textures/particles/generic/water/splash"
      }
    },
    "components": {
      "minecraft:emitter_rate_steady": {
        "spawn_rate": 100,
        "max_particles": 200
      },
      "minecraft:emitter_lifetime_once": {
        "active_time": 1
      },
      "minecraft:emitter_shape_point": {
        "direction": [
          "math.random(-3.01,3.01)",
          "math.random(-0.91,3.01)",
          "math.random(-2.99,0.31)"
        ]
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": 0.25
      },
      "minecraft:particle_initial_spin": {
        "rotation": "math.random_integer(0,3)*90"
      },
      "minecraft:particle_initial_speed": "math.random(5.25,9.25)*0.6",
      "minecraft:particle_motion_dynamic": {
        "linear_acceleration": [
          0,
          -6,
          0
        ],
        "linear_drag_coefficient": 2
      },
      "minecraft:particle_appearance_billboard": {
        "size": [
          "0.1+(v.particle_age*0.05)",
          "0.066+(v.particle_age*0.05)"
        ],
        "facing_camera_mode": "rotate_xyz",
        "uv": {
          "texture_width": 12,
          "texture_height": 2,
          "uv": [
            "math.round(v.particle_random_3)*3",
            0
          ],
          "uv_size": [
            3,
            2
          ]
        }
      },
      "minecraft:particle_motion_collision": {
        "expire_on_contact": true
      },
      "minecraft:particle_appearance_lighting": {},
      "minecraft:particle_appearance_tinting": {
        "color": {
          "interpolant": "v.particle_age / v.particle_lifetime",
          "gradient": {
            "0.0": "#FFFFFFFF",
            "0.71": "#FFFFFFFF",
            "0.93": "#00FFFFFF"
          }
        }
      }
    }
  }
}
