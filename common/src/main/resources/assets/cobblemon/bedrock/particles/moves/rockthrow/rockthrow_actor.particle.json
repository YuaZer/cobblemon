{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:rockthrow_actor",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/impact/impact_rock"
			}
		},
		"curves": {
			"variable.rsize": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1, 1, 1, 1, 1, 1, 1]
			}
		},
		"events": {
			"break": {
				"particle_effect": {
					"effect": "cobblemon:rockthrow_target",
					"type": "particle"
				}
			},
			"send": {
				"particle_effect": {
					"effect": "cobblemon:rockthrow_actorsend",
					"type": "emitter"
				}
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "v.target_deltaz=5;"
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.25
			},
			"minecraft:emitter_lifetime_events": {
				"creation_event": "send"
			},
			"minecraft:emitter_shape_disc": {
				"offset": [0, 0.02, 0.1],
				"radius": 0.5,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.55
			},
			"minecraft:particle_lifetime_events": {
				"timeline": {
					"0.47": "break"
				}
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0,3)*90"
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": ["-v.target_deltax*(v.particle_age / v.particle_lifetime)", "(v.target_deltay*(v.particle_age / v.particle_lifetime))+math.sin(v.particle_age*340)*0.6", "-v.target_deltaz*(v.particle_age / v.particle_lifetime)"]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.275*v.rsize", "0.275*v.rsize"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 56,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 16,
						"max_frame": 7,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_lighting": {}
		}
	}
}