{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:leechseed_actor",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/grass/xsseed"
			}
		},
		"curves": {
			"variable.seedarc": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.52, 0.84, 0.96, 1, 0.98, 0.87, 0.64, 0]
			}
		},
		"events": {
			"impact": {
				"particle_effect": {
					"effect": "cobblemon:leechseed_target",
					"type": "particle"
				}
			},
			"sprout": {
				"particle_effect": {
					"effect": "cobblemon:leechseed_sprout",
					"type": "particle"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 7,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.6
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.6
			},
			"minecraft:particle_lifetime_events": {
				"expiration_event": ["impact", "sprout"]
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0,3)*90"
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": ["-(v.target_deltax+(v.particle_random_1-0.5)*1) * (v.particle_age / v.particle_lifetime)", "(v.target_deltay+(v.particle_random_2-0.5)*1) * (v.particle_age / v.particle_lifetime)+(v.seedarc*0.5)", "-(v.target_deltaz-(v.particle_random_1-0.5)*0.15) * (v.particle_age / v.particle_lifetime)"]
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.125, 0.125],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 56,
					"texture_height": 7,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [7, 7],
						"step_UV": [7, 0],
						"frames_per_second": 16,
						"max_frame": 8,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_lighting": {}
		}
