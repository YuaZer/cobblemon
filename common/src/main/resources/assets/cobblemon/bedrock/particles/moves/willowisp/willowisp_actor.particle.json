{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:willowisp_actor",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/fire/cloudyfire_white"
			}
		},
		"curves": {
			"variable.fun": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [1, 1, 1, 1, 0.97, 0.87, 0.54, 0]
			}
		},
		"events": {
			"appear": {
				"particle_effect": {
					"effect": "cobblemon:willowisp_actorappear",
					"type": "particle"
				}
			},
			"wisp": {
				"particle_effect": {
					"effect": "cobblemon:willowisp_actorwisp",
					"type": "particle"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 24,
				"max_particles": 6
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.3
			},
			"minecraft:emitter_lifetime_events": {
				"creation_event": "appear"
			},
			"minecraft:emitter_shape_point": {
				"direction": ["math.sin(v.emitter_age*3100)", "math.cos(v.emitter_age*3100)", "math.random(-0.85,-0.35)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.65
			},
			"minecraft:particle_lifetime_events": {
				"timeline": {
					"0.45": "appear",
					"0.48": "wisp"
				}
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.round(v.particle_random_4*3)*90"
			},
			"minecraft:particle_initial_speed": "math.random(6.5,8.5)*1.1",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["(v.particle_random_1-0.5)*4", "4+(v.particle_random_1-0.5)*3", "(v.particle_random_1-0.5)*3"],
				"linear_drag_coefficient": 4
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(0.5-(v.particle_random_4*0.15))*v.fun", "(0.5-(v.particle_random_4*0.15))*v.fun"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 160,
					"texture_height": 16,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [16, 16],
						"step_UV": [16, 0],
						"frames_per_second": 20,
						"max_frame": 10
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"coefficient_of_restitution": 2,
				"collision_radius": 0.01
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.1": "#FFC1DAFF",
						"0.35": "#FF708FFF",
						"0.65": "#FF5466FF",
						"1.0": "#FF3B4FFF"
					}
				}
			}
		}
	}
}