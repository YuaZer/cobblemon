{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:hornattack_actor",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/spike"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_point": {
				"direction": ["-v.target_deltax", "v.target_deltay", "-v.target_deltax"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.25
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": ["(-v.target_deltax * (v.particle_age / v.particle_lifetime))", "(v.target_deltay * (v.particle_age / v.particle_lifetime))", "(-v.target_deltaz * (v.particle_age / v.particle_lifetime))"],
				"direction": ["-v.target_deltax", "v.target_deltay", "-v.target_deltaz"]
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.85, 0.85],
				"facing_camera_mode": "lookat_direction",
				"uv": {
					"texture_width": 256,
					"texture_height": 32,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [32, 32],
						"step_UV": [32, 0],
						"frames_per_second": 20,
						"max_frame": 6,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.28": "#ffffffff",
						"0.57": "#FFFEFF91",
						"0.77": "#FFFEFF91",
						"0.88": "#FFFF944B"
					}
				}
			}
		}
	}
}