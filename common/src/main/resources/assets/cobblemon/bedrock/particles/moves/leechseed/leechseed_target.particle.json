{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:leechseed_target",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/orb/xsfadeorblite"
			}
		},
		"curves": {
			"variable.upwards": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1080]
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 30,
				"max_particles": 4
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.4
			},
			"minecraft:emitter_shape_point": {
				"offset": [0, 0, 0],
				"direction": ["math.sin(v.emitter_age*3000)*0.75", "(v.particle_random_2-0.25)*2", "math.cos(v.emitter_age*3000)*0.75"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_speed": "math.random(3.51,4.51)*0.85",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, -4, 0],
				"linear_drag_coefficient": 3
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.125, 0.125],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 45,
					"texture_height": 5,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [5, 5],
						"step_UV": [5, 0],
						"frames_per_second": 18,
						"max_frame": 9
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FFCEFFAB",
						"0.7": "#FF4BB538"
					}
				}
			}
		}
	}
}