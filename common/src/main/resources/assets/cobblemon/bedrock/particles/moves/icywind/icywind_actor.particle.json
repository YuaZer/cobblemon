{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:icywind_actor",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/generic/impact/impact_ice"
			}
		},
		"curves": {
			"variable.iwsizing": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.53, 0.84, 1, 1, 1, 1, 1, 1, 1]
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "v.target_deltaz=5;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": "25*math.clamp(v.target_deltaz,5,7)",
				"max_particles": 1000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1.1
			},
			"minecraft:emitter_shape_point": {
				"direction": ["-v.target_deltax+((v.particle_random_1-0.5)*1.2)*math.clamp(v.target_deltaz,2.5,5)", "v.target_deltay+(v.particle_random_2-0.5)*math.clamp(v.target_deltaz,2.5,5)", "-math.clamp(v.target_deltaz,2.5,5)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(0.9,1.5)*1.1"
			},
			"minecraft:particle_initial_speed": "math.random(1.25,2.25)*math.clamp(v.target_deltaz,2.5,7)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.sin(v.particle_age*((v.particle_random_2-0.5)*(360+(v.particle_random_2*180))))*1.5", "math.cos(v.particle_age*((v.particle_random_3-0.5)*(360+(v.particle_random_2*180))))*1.5", "math.sin(v.particle_age*((v.particle_random_4-0.5)*(360+(v.particle_random_2*180))))*1.5"],
				"linear_drag_coefficient": "0.3*math.clamp(v.target_deltaz,2.5,5)"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.1*v.iwsizing", "0.1*v.iwsizing"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 56,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 10,
						"max_frame": 7,
						"loop": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.2,
				"coefficient_of_restitution": 0.2,
				"collision_radius": 0.1
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#00316BFF",
						"0.1": "#BFD7EBFF",
						"0.5": "#BFA5D2FF",
						"0.7": "#BF8BC4FF",
						"1.0": "#00316BFF"
					}
				}
			}
		}
	}
}