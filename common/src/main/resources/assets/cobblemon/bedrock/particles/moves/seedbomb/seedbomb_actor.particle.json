{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:seedbomb_actor",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/grass/seed"
			}
		},
		"curves": {
			"variable.seedarc": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.52, 0.84, 0.96, 1, 0.94, 0.78, 0.48, 0]
			}
		},
		"events": {
			"burst": {
				"particle_effect": {
					"effect": "cobblemon:seedbomb_target",
					"type": "particle"
				}
			},
			"boom": {
				"particle_effect": {
					"effect": "cobblemon:seedbomb_targetboom",
					"type": "particle"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 6,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.7
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.7
			},
			"minecraft:particle_lifetime_events": {
				"timeline": {
					"0.67": ["burst", "boom"]
				}
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0,3)*90"
			},
			"minecraft:particle_motion_parametric": {
