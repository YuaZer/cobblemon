				"size": [0.2, 0.2],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 72,
					"texture_height": 9,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [9, 9],
						"step_UV": [9, 0],
						"frames_per_second": 16,
						"max_frame": 8,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_lighting": {}
		}
