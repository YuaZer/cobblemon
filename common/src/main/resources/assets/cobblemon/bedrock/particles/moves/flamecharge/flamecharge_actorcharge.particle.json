{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:flamecharge_actorcharge",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/orb/xsfadeorblite"
			}
		},
		"curves": {
			"variable.fun": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.75, 0.95, 1, 0.96, 0.86, 0.69, 0.4, 0]
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "v.entity_height=1;v.entity_width=1;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 60,
				"max_particles": 300
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.9
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.round(v.particle_random_4*3)*90"
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": ["(math.cos(v.particle_age*(120-(q.entity_height*8))+(v.particle_random_1*360))*(math.clamp(q.entity_radius*0.9,0.2,4)+(v.particle_age*0.7)))", "-0.05+(v.particle_random_1*0.25)+(v.particle_age*((math.clamp(q.entity_height*2.5,1.5,5))*(v.particle_random_4+0.5)))+(v.particle_random_2*0.5))", "(math.sin(v.particle_age*(120-(q.entity_height*8))+(v.particle_random_1*360))*(math.clamp(q.entity_radius*0.9,0.2,4)+(v.particle_age*0.7)))"],
				"direction": [0, 1, 0]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.06*v.fun", "0.06*v.fun"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 45,
					"texture_height": 5,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [5, 5],
						"step_UV": [5, 0],
						"frames_per_second": 16,
						"max_frame": 9,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.2": "#FFFFE872",
						"0.4": "#FFFFCC00",
						"0.55": "#FFFF9400",
						"0.75": "#FFFF4700",
						"0.9": "#FFFF4700",
						"1.0": "#FF531C0E"
					}
				}
			}
		}
	}
}