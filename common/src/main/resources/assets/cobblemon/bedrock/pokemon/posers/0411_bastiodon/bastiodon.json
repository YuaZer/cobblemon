{
  "portraitScale": 1.4,
  "portraitTranslation": [-0.75, -0.2, 0],
  "profileScale": 0.7,
  "profileTranslation": [0, 0.6, 0],
  "rootBone": "bastiodon",
  "firstPersonCameraOffset": {
    "seat_1": [0, 0.5, 0]
  },
  "animations": {
    "faint": "q.bedrock_primary('bastiodon', 'faint')",
    "cry": "q.bedrock_stateful('bastiodon', 'cry')"
  },
  "poses": {
    "battle-standing": {
      "poseTypes": ["STAND"],
      "isBattle": true,
      "animations": [
        "q.look('head')",
        "q.bedrock('bastiodon', 'battle_idle')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": [
        "q.bedrock_quirk('bastiodon', 'blink')"
      ]
    },
    "standing": {
      "poseTypes": ["STAND", "NONE", "PORTRAIT", "PROFILE"],
      "isBattle": false,
      "condition": "!q.in_air",
      "animations": [
        "q.look('head')",
        "q.bedrock('bastiodon', 'ground_idle')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": ["q.bedrock_quirk('bastiodon', 'blink')"]
    },
    "walking": {
      "poseTypes": ["WALK", "FLOAT", "SWIM"],
      "condition": "!q.is_sprinting && !q.is_air && q.has_entity",
      "animations": [
        "q.look('head')",
        "q.bedrock('bastiodon', 'ground_walk')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": ["q.bedrock_quirk('bastiodon', 'blink')"]
    },
    "running": {
      "poseTypes": ["WALK", "FLOAT", "SWIM"],
      "isBattle": false,
      "condition": "!q.in_air && q.is_sprinting && q.is_ridden && q.has_entity",
      "animations": [
        "q.look('head')",
        "q.bedrock('bastiodon', 'ground_run')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": ["q.bedrock_quirk('bastiodon', 'blink')"]
    },
    "jumping": {
      "poseTypes": ["WALK","STAND", "FLOAT", "NONE"],
      "isBattle": false,
      "condition": "q.in_air && q.is_ridden && q.has_entity",
      "animations": [
        "q.look('head')",
        "q.bedrock_primary('bastiodon','ride_jump')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": ["q.bedrock_quirk('bastiodon', 'blink')"],
      "transformTicks": 5,
      "transformToTicks": 5
    },
    "sleep": {
      "poseTypes": ["SLEEP"],
      "namedAnimations": {
        "cry": "q.bedrock_stateful('dummy', 'cry')"
      },
      "animations": [
        "q.bedrock('bastiodon', 'sleep')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('bastiodon', 'hold_item')"
        }
      ],
      "quirks": ["q.bedrock_primary_quirk('bastiodon', 'sleep_snore_quirk', 10, 25, 1, q.curve('symmetrical_wide'))"]
    }
  }
}
