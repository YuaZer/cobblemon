      {
        "condition": "q.is_holding_item",
        "animation": "q.bedrock('ursaluna', 'hold_item')"
      }
    ],
    "transformTicks": 10
    },
    "walk": {
      "poseName": "walk",
      "poseTypes": ["WALK", "SWIM"],
      "isBattle": false,
      "condition": "!q.in_air && !q.is_sprinting",
      "animations": [
        "q.look('head')",
        "q.bedrock('ursaluna', 'ground_walk')",
       
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('ursaluna', 'hold_item')"
        },
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('ursaluna', 'ride_ground_walk')"
        }
      ],
      "transformTicks": 10,
      "quirks": [
        "q.bedrock_quirk('ursaluna', 'blink')"
      ]
    },
    "run": {
      "poseName": "run",
      "poseTypes": ["WALK"],
      "isBattle": false,
      "condition": "!q.in_air && q.is_sprinting",
      "animations": [
        "q.look('head')",
        "q.bedrock('ursaluna', 'ground_run')",
        {
          "condition": "q.is_holding_item",
          "animation": "q.bedrock('ursaluna', 'hold_item')"
        },
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('ursaluna', 'ride_ground_run')"
        }
