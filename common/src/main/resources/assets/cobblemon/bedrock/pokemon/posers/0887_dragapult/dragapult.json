{
  "portraitScale": 1.8,
  "portraitTranslation": [-0.25, 3.3, 0],
  "profileScale": 0.45,
  "profileTranslation": [0.0, 1.25, 0],
  "rootBone": "dragapult",
  "firstPersonCameraOffset": {
    "seat_1": [0, 0.45, 0]
  },
  "thirdPersonCameraOffset": {
    "seat_1": [0, 0.4, 0],
    "seat_1_reverse": [0, 0.4, 0]
  },
  "thirdPersonCameraOffsetNoViewBobbing": {
    "seat_1": [0, 0.4, 0],
    "seat_1_reverse": [0, 0.4, 0]
  },
  "animations": {
    "cry": "q.bedrock_stateful('dragapult', 'cry')",
    "recoil": "q.bedrock_stateful('dragapult', 'recoil')",
    "physical": "q.bedrock_primary('dragapult', 'physical', q.curve('symmetrical_wide'))",
    "special": "q.bedrock_primary('dragapult', 'special', q.curve('symmetrical_wide'))"
  },
  "poses": {
    "battle-standing": {
      "poseTypes": ["STAND"],
      "isBattle": true,
      "animations": ["q.pitch_tilt('body')","q.look('head')", "q.bedrock('dragapult', 'battle_idle')"],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ]
    },
    "standing": {
      "poseTypes": ["STAND", "NONE", "PORTRAIT", "PROFILE"],
      "isBattle": false,
      "animations": ["q.pitch_tilt('body')","q.look('head')", "q.bedrock('dragapult', 'ground_idle')"],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ]
    },
    "walking": {
      "poseTypes": ["WALK"],
      "condition": "!q.in_air&& !q.is_sprinting",
      "animations": ["q.pitch_tilt('body')","q.look('head')", "q.bedrock('dragapult', 'ground_walk')"],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ],
        "transformTicks": 10
    },
      "run": {
        "poseTypes": ["STAND","WALK"],
        "isBattle": false,
        "condition": "!q.in_air && q.is_sprinting",
        "animations": [
          "q.look('head')",
          "q.bedrock('dragapult', 'ground_run')",
        {
          "condition": "q.is_ridden",
          "animation": "q.bedrock('dragapult', 'ride_ground_run')"
        }
        ],
        "quirks": [
          "q.bedrock_quirk('dragapult', 'blink')"
        ],
        "transformTicks": 10
      },
    "sleep": {
      "poseTypes": ["SLEEP"],
      "animations": ["q.bedrock('dragapult', 'sleep')"]
    },
    "hover": {
      "poseTypes": ["HOVER"],
      "isBattle": false,
      "animations": [
        "q.pitch_tilt('body')",
        "q.look('head')",
        "q.bedrock('dragapult', 'ground_idle')"
      ],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ]
    },
    "fly": {
      "poseTypes": ["FLY"],
      "isBattle": false,
      "condition": "!q.is_gliding",
      "animations": [
        "q.pitch_tilt('body')",
        "q.bedrock('dragapult', 'air_fly')",
        {
          "condition": "q.is_ridden",
          "animation": "q.bedrock('dragapult', 'ride_air_fly')"
        }
      ],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ],
        "transformTicks": 10
    },
    "glide": {
      "poseTypes": ["FLY"],
      "isBattle": false,
      "condition": "q.is_gliding",
      "animations": [
        "q.look('head')",
        "q.bedrock('dragapult', 'air_glide')",
        {
          "condition": "q.is_ridden",
          "animation": "q.bedrock('dragapult', 'ride_air_fly')"
        }
      ],
      "quirks": [
        "q.bedrock_quirk('dragapult', 'blink')",
        "q.bedrock_quirk('dragapult', 'blink2')",
        "q.bedrock_quirk('dragapult', 'blink3')"
      ],
        "transformTicks": 10
    }
  }
}
